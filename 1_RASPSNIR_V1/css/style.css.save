
html, body
{
	height : 100%;
}
nav {
    background-color: rgb(81, 119, 158);
}

#nav-mobile {
    padding-left: 10%;
}

#top-image {
background:url('http://matthewtiller.com/wp-content/uploads/2015/09/code-wallpaper-8238-8558-hd-wallpapers.jpg') -25px -50px;
position:fixed ;
top:-5px;
    left: -200px;
width:130%;
z-index:-1;
  height:110%;
  background-size: calc(100% + 50px);
  -webkit-filter: blur(5px);
  -moz-filter: blur(5px);
  -o-filter: blur(5px);
  -ms-filter: blur(5px);
  filter: blur(5px);
    
}

.dropdown-content {
    background-color: #284A6F;
}

.dropdown-content:hover {
    background-color: #284A6F;
}


.dropdown-content li>a {
    color: white;
}

.card .card-action a:not(.btn):not(.btn-large):not(.btn-floating) {
    color: #284A6F;
}
.card .card-action a:not(.btn):not(.btn-large):not(.btn-floating):hover {
    color: rgb(81, 119, 158);
}

.collapsible-body {
    background-color: lightgrey;
}

#n1 {
    color: gold;
}

#n2 {
    color: silver;
}

.collapsible-body > img {
    margin: 0 auto;
}
#siteSection
{
	width : 100%;
	height : 100%;
}
